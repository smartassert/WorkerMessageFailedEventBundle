{
    "name": "smartassert/worker-message-failed-event-bundle",
    "description": "",
    "keywords": [],
    "homepage": "https://github.com/smartassert/WorkerMessageFailedEventBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "webignition@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SmartAssert\\WorkerMessageFailedEventBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SmartAssert\\WorkerMessageFailedEventBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "cs-fixer-dry-run": "./vendor/bin/php-cs-fixer check --allow-risky=yes --verbose --show-progress none",
        "cs-fixer-fix": "./vendor/bin/php-cs-fixer fix --allow-risky=yes --diff",
        "cs-phpcs": "XDEBUG_MODE=off ./vendor/bin/phpcs",
        "cs": [
            "@cs-fixer-dry-run",
            "@cs-phpcs"
        ],
        "static-analysis": "XDEBUG_MODE=off ./vendor/bin/phpstan analyse --ansi",
        "test": "XDEBUG_MODE=off ./vendor/bin/phpunit --colors=always tests"
    },
    "require": {
        "php": "^8.4",
        "symfony/config": "^7 || ^8",
        "symfony/dependency-injection": "^7 || ^8",
        "symfony/messenger": "^7 || ^8",
        "symfony/yaml": "^7 || ^8"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.91",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpunit/phpunit": "^10.1",
        "squizlabs/php_codesniffer": "^4.0",
        "symfony/event-dispatcher": "^7 || ^8",
        "symfony/http-kernel": "^7 || ^8"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
